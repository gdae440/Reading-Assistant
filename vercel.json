{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/siliconflow/(.*)",
      "dest": "https://api.siliconflow.cn/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
